{
  "name": "detlef-pwa",
  "description": "Progressive Web App",
  "author": "Dalibor Gogic <mail@dalibrgogic.com>",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"node -r dotenv/config\" server -w server.js",
    "start": "NODE_ENV=production node -r dotenv/config server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "contentful": "^4.5.0",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "intersection-observer": "^0.3.2",
    "lru-cache": "^4.0.2",
    "marked": "^0.3.6",
    "serve-favicon": "^2.4.1",
    "vue": "^2.3.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.3.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.1",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-rc.2",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
